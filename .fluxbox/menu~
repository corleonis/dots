[begin] (fluxbox)
[exec] (Notes) {everpad}
[exec] (Files) {thunar}
[exec] (Search) {catfish}
[exec] (Music) {rhythmbox}
[submenu] (Media) {}
	[exec] (Transmission BT) {transmission-gtk}
	[exec] (VLC Media Player) {vlc}
	[exec] (Totem Media Player) {totem}
	[exec] (Shotwell) {shotwell}
	[exec] (Drum Machine) {hydrogen}
[end]
[exec] (Web) {firefox} 
[exec] (Skype) {skype}
[exec] (Virtual Machines) {virtualbox}
[submenu] (System) {}
	[reconfig] ([Reload fluxbox config])
	[submenu] (Default fluxbox menu) {}
		[include] (/etc/X11/fluxbox/fluxbox-menu)
	[end]
	[exec] (GNOME Settings) {gnome-control-center}
	[exec] (gtk-chtheme) {gtk-chtheme}
	[exec] (Battery Data) {battery-graph}
[end]
[submenu] (Power) {}
	[exec] (Logout) {exit}
	[exec] (Suspend) {gksudo pm-suspend}
	[exec] (Reboot) {gksudo reboot}
	[exec] (Halt) {gksudo halt}
[end]
[end]
